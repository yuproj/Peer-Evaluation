MAIL_SERVER=smtp.gmail.com
MAIL_PORT=587
MAIL_USERNAME=your-email@monmouth.edu
MAIL_PASSWORD=your-app-specific-password
MAIL_DEFAULT_SENDER=noreply@peerevaluation.com
